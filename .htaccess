<IfModule mod_rewrite.c>
	Options +FollowSymlinks
	RewriteEngine On
	
	RewriteCond %{REQUEST_URI} ^\/d\/([a-zA-Z0-9_]+)\/([a-zA-Z0-9_]+)(.*)$
	RewriteRule .* http://depman.local/index.php?c=%1&m=%2%3 [R=302,QSA]
	
	RewriteCond %{REQUEST_URI} ^\/a\/([a-zA-Z0-9_]+)\/([a-zA-Z0-9_]+)(.*)+?$ 
	RewriteRule .* http://depman.local/index.php?x=1&c=%1&m=%2%3 [R=302,QSA]
	
</IfModule>
